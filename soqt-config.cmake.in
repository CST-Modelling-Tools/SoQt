set(SoQt_VERSION "@SOQT_MAJOR_VERSION@.@SOQT_MINOR_VERSION@.@SOQT_MICRO_VERSION@")
set(SoQt_VERSION_MAJOR "@SOQT_MAJOR_VERSION@")
set(SoQt_VERSION_MINOR "@SOQT_MINOR_VERSION@")
set(SoQt_VERSION_PATCH "@SOQT_MICRO_VERSION@")

@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
include(GNUInstallDirs)

find_dependency(Coin)

if(@Qt5_FOUND@)
	find_dependency(Qt5Core)
	find_dependency(Qt5Gui)
	find_dependency(Qt5OpenGL)
	find_dependency(Qt5Widgets)
else(@Qt4_FOUND@)
	find_dependency(Qt4)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/soqt-export.cmake")

if(SIMAGE_BUILD_SHARED_LIBS)
	set(SoQt_DEFINITIONS "-DSOQT_DLL")
else()
	set(SoQt_DEFINITIONS "-DSOQT_NOT_DLL")
endif()

set(SoQt_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/${CMAKE_INSTALL_INCLUDEDIR}")
set(SoQt_LIBRARY_DIRS "${PACKAGE_PREFIX_DIR}/${CMAKE_INSTALL_LIBDIR}")
set(SoQt_LIBRARIES "SoQt::SoQt")
