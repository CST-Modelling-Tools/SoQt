## Process this file with automake to generate Makefile.in.

if BUILD_WITH_MSVC
noinst_LIBRARIES = viewers.lst
else
noinst_LTLIBRARIES = libSoQtViewers.la
endif

## Remove any linklibraries detected by configure, as they are not
## needed in the submodules -- only in the toplevel libSoQt build.

INCLUDES = -I$(top_srcdir)/src

LIBS = 

############################################################################
## Autogenerated source files (some by the Qt moc utility; necessary to get
## the Qt signal-slot mechanisms working).

BUILT_SOURCES = \
	SoQtViewer.h \
	SoQtViewer.cpp \
	SoQtConstrainedViewer.h \
	SoQtConstrainedViewer.cpp \
	SoQtFlyViewer.h \
	SoQtFlyViewer.cpp \
	SoAnyFullViewer.h \
	SoAnyFullViewer.cpp \
	SoAnyExaminerViewer.h \
	SoAnyExaminerViewer.cpp \
	SoAnyPlaneViewer.h \
	SoAnyPlaneViewer.cpp \
	moc_SoQtViewer.cpp \
	moc_SoQtFullViewer.cpp \
	moc_SoQtExaminerViewer.cpp \
	moc_SoQtPlaneViewer.cpp \
	moc_SoQtConstrainedViewer.cpp \
	moc_SoQtWalkViewer.cpp \
	moc_SoQtFlyViewer.cpp

DISTCLEANFILES = \
	$(BUILT_SOURCES)

