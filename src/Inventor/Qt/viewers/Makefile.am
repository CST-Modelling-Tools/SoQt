############################################################################
# libtool setup

libSoQtViewers_la_SOURCES = \
	SoQtFullViewer.cpp \
	SoQtFullViewer.h \
	SoQtExaminerViewer.cpp \
	SoQtExaminerViewer.h \
	SoQtPlaneViewer.cpp \
	SoQtPlaneViewer.h \
	SoQtWalkViewer.cpp \
	SoQtWalkViewer.h

nodist_libSoQtViewers_la_SOURCES = \
	$(BUILT_SOURCES)

############################################################################
# custom setup

viewers_lst_SOURCES = \
	SoQtFullViewer.cpp \
	SoQtFullViewer.h \
	SoQtExaminerViewer.cpp \
	SoQtExaminerViewer.h \
	SoQtPlaneViewer.cpp \
	SoQtPlaneViewer.h \
	SoQtWalkViewer.cpp \
	SoQtWalkViewer.h

nodist_viewers_lst_SOURCES = \
	$(BUILT_SOURCES)

viewers.lst: Makefile $(viewers_lst_OBJECTS)
	@echo "Linking viewers.lst..."; \
	rm -f $@; \
	for i in $(viewers_lst_OBJECTS); do echo $$i >>$@; done

############################################################################
# header installation

SoQtViewersincdir = $(includedir)/Inventor/Qt/viewers
SoQtViewersinc_HEADERS = \
	$(top_srcdir)/src/Inventor/Qt/viewers/SoQtFullViewer.h \
	$(top_srcdir)/src/Inventor/Qt/viewers/SoQtExaminerViewer.h \
	$(top_srcdir)/src/Inventor/Qt/viewers/SoQtPlaneViewer.h \
	$(top_srcdir)/src/Inventor/Qt/viewers/SoQtWalkViewer.h

nodist_SoQtViewersinc_HEADERS = \
	$(top_builddir)/src/Inventor/Qt/viewers/SoQtViewer.h \
	$(top_builddir)/src/Inventor/Qt/viewers/SoQtConstrainedViewer.h \
	$(top_builddir)/src/Inventor/Qt/viewers/SoQtFlyViewer.h

install-SoQtViewersincHEADERS: $(SoQtViewersinc_HEADERS)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(SoQtViewersincdir)
	@list='$(SoQtViewersinc_HEADERS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  src="$$d$$p"; dst="$(DESTDIR)$(SoQtViewersincdir)/$$f"; \
	  if cmp -s "$$src" "$$dst"; then :; else \
	    echo " $(INSTALL_HEADER) $$src $$dst"; \
	    $(INSTALL_HEADER) "$$src" "$$dst"; \
	  fi; \
	done

install-nodist_SoQtViewersincHEADERS: $(nodist_SoQtViewersinc_HEADERS)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(SoQtViewersincdir)
	@list='$(nodist_SoQtViewersinc_HEADERS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  src="$$d$$p"; dst="$(DESTDIR)$(SoQtViewersincdir)/$$f"; \
	  if cmp -s "$$src" "$$dst"; then :; else \
	    echo " $(INSTALL_HEADER) $$src $$dst"; \
	    $(INSTALL_HEADER) "$$src" "$$dst"; \
	  fi; \
	done

